<%= form_with url: "/patrons/#{@patron.id}", method: :patch, local: true do |form|%>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :years_member %>
  <%= form.text_field :years_member %>
  <%= form.label :belay_certified %>
  <%= form.text_field :belay_certified %>
  <%= form.submit "Update Patron" %>
<%end%>